HEADDATE=`svn info | grep 'Last Changed Date' | cut -c 20-38`
perl -pi -e s/.WCREV./$HEADREV/g SvnInfo.cs
perl -pi -e s/.WCDATE./"$HEADDATE"/g SvnInfo.cs
cd $(OutDir)</PreBuildEvent>
